<?xml version="1.0" encoding="UTF-8"?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="settleMetal.Main">
  <MasterDetailPage.Master>
    <ContentPage Padding="10" Title="Master" BackgroundColor="Gray">
      <ContentPage.Content>
        <StackLayout Margin="5,30,5,5">
          <Label Text="Master"></Label>
          <Button Text="Home" Clicked="home"></Button>
          <Button Text="Login" Clicked="loginUser"></Button>
          <Button Text="Register" Clicked="registerUser"></Button>
        </StackLayout>
      </ContentPage.Content>
    </ContentPage>
  </MasterDetailPage.Master>
  

  <MasterDetailPage.Detail>
      <ContentPage Padding="10" Title="Master" BackgroundColor="Gray">
      <ContentPage.Content>
        <StackLayout Margin="5,30,5,5">
          <Label Text="Detail"></Label>
          
        </StackLayout>
      </ContentPage.Content>
    </ContentPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>
